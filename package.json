{
    "name": "library-express-demo",
    "version": "1.0.0",
    "description": "Demo based on Pluralsight course https://app.pluralsight.com/library/courses/nodejs-express-web-applications-update/table-of-contents.",
    "main": "index.js",
    "dependencies": {
        "bootstrap": "4.4.1",
        "chalk": "3.0.0",
        "debug": "4.1.1",
        "express": "4.17.1",
        "jquery": "3.4.1",
        "morgan": "1.9.1",
        "popper.js": "1.16.1"
    },
    "devDependencies": {
        "eslint": "6.8.0",
        "eslint-config-airbnb-base": "14.0.0",
        "eslint-plugin-import": "2.20.1",
        "nodemon": "^2.0.2"
    },
    "scripts": {
        "prestart:debug": "npm run lint",
        "start:debug": "set DEBUG=app & nodemon app.js",
        "lint": "eslint app.js"
    },
    "repository": {
        "type": "git",
        "url": "git+https://github.com/luckpp/library-express-demo.git"
    },
    "author": "luckpp",
    "license": "ISC",
    "bugs": {
        "url": "https://github.com/luckpp/library-express-demo/issues"
    },
    "homepage": "https://github.com/luckpp/library-express-demo#readme",
    "nodemonConfig": {
        "restartable": "rs",
        "ignore": [
            "node_modules/**/node_modules"
        ],
        "delay": "2500",
        "env": {
            "DEBUG": "app",
            "NODE_ENV": "development",
            "PORT": "3000"
        }
    }
}
